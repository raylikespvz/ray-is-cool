local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "ArmWrestlingSimulator", HidePremium = false, IntroEnabled = true, IntroText = "Niggers", SaveConfig = true, ConfigFolder = "OrionTest"})

local NiggaTab = Window:MakeTab({

